{
  "name": "brgestor-api-service",
  "version": "2.0.0",
  "description": "API REST que substitui Supabase - autenticação, webhooks e operações de banco",
  "main": "api-service.js",
  "scripts": {
    "start": "node api-service.js",
    "dev": "nodemon api-service.js",
    "pm2:start": "pm2 start api-service.js --name brgestor-api --env production",
    "pm2:stop": "pm2 stop brgestor-api",
    "pm2:restart": "pm2 restart brgestor-api",
    "pm2:logs": "pm2 logs brgestor-api"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "api",
    "rest",
    "brgestor",
    "authentication",
    "webhook"
  ],
  "author": "BR Gestor",
  "license": "MIT"
}