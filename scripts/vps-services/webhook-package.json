{
  "name": "brgestor-webhook-service",
  "version": "1.0.0",
  "description": "Webhook service para substituir Edge Functions do Supabase",
  "main": "webhook-service.js",
  "scripts": {
    "start": "node webhook-service.js",
    "dev": "nodemon webhook-service.js",
    "pm2:start": "pm2 start webhook-service.js --name brgestor-webhook --env production",
    "pm2:stop": "pm2 stop brgestor-webhook",
    "pm2:restart": "pm2 restart brgestor-webhook",
    "pm2:logs": "pm2 logs brgestor-webhook"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "webhook",
    "whatsapp",
    "brgestor",
    "api"
  ],
  "author": "BR Gestor",
  "license": "MIT"
}